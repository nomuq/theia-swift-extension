{
  "name": "@theia/swift",
  "description": "Theia - Swift Extension",
  "license": "Apache-2.0",
  "keywords": [
    "theia-extension"
  ],
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "files": [
